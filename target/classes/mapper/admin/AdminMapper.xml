<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.orrange.admin.mapper.AdminMapper">
    <select id="selectByPhone" parameterType="string" resultType="com.orrange.admin.entity.Admin">
        SELECT id, phone FROM admin WHERE phone = #{phone}
    </select>
    
    <insert id="insertAdmin" parameterType="com.orrange.admin.entity.Admin">
        INSERT INTO admin (phone, password) VALUES (#{phone}, #{password})
    </insert>
</mapper> 